   "source": [
    "# Regular expression for URLs (covers http and https)\n",
    "url_pattern = r'http[s]?://(?:[a-zA-Z]|[0-9]|[$-_@.&+]|[!*\\(\\),]|(?:%[0-9a-fA-F][0-9a-fA-F]))+'\n",
    "\n",
    "# Count URLs in each row of the 'body' column\n",
    "df['url'] = df['body'].apply(lambda x: len(re.findall(url_pattern, str(x))))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "d96d08b2",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Regular expression for URLs\n",
    "url_pattern = r'https?://\\S+|www\\.\\S+'\n",
    "\n",
    "def extract_urls(text):\n",
    "    urls = re.findall(url_pattern, text)\n",
    "    return ','.join(urls) if urls else ''\n",
    "\n",
    "df['url_names'] = df['body'].apply(extract_urls)"
   ]
  },
